<!-- 마이페이지 -->
<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zVXn8CA5QIVfZOJ3BCsw2P0p/We" crossorigin="anonymous">

  <title>My page</title>
</head>

<body class="text-center" style="margin-top: 30px; margin-left: 50px; margin-right: 50px;">
  <div class="px-4 py-5 my-5 text-center">
    <h1 class="display-5 fw-bold">배송지</h1>
    <div class="col-lg-10 mx-auto">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">번호</th>
            <th scope="col">우편번호</th>
            <th scope="col">주소</th>
            <th scope="col">상세주소</th>
            <th scope="col">수정</th>
            <th scope="col">삭제</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0; i < delivery.length; i++) {%>
            <tr>
              <th scope="row">
                <p class="card-text">
                  <%= i+1 %>
                </p>
              </th>
              <td>
                <%=delivery[i].delivery_code %></a>
              </td>
              <td>
                <%=delivery[i].delivery_adress %>
              </td>
              <td>
                <%=delivery[i].delivery_detail %>
              </td>
              <td>
                <div class="btn-group" role="group" aria-label="Basic mixed styles example">
                  <button type="button" class="btn btn-primary" style="font-size: small;"
                    onclick="location.href='/mypage/ship/update/<%= delivery[i].delivery_uid %>'">수정</button>
                </div>
              </td>
              <td>
                <div class="btn-group" role="group" aria-label="Basic mixed styles example">
                  <form action='/mypage/ship/delete/<%= delivery[i].delivery_uid %>?_method=delete' method='post'>
                    <button type="submit" class="btn btn-outline-primary" style="font-size: small;">삭제</button>
                  </form>
                </div>
              </td>
            </tr>
            <% } %>
        </tbody>
      </table>
      <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
        <button type="button" class="btn btn-dark btn-lg px-4 gap-3"
          onclick="location.href='/mypage/ship/insert'">추가</button>
      </div>
    </div>
  </div>

  <div class="px-4 py-5 my-5 text-center">
    <h1 class="display-5 fw-bold">카드</h1>
    <div class="col-lg-10 mx-auto">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">번호</th>
            <th scope="col">카드 번호</th>
            <th scope="col">카드 종류</th>
            <th scope="col">카드 유효기간</th>
            <th scope="col">수정</th>
            <th scope="col">삭제</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0; i < card.length; i++) {%>
            <tr>
              <th scope="row">
                <p class="card-text">
                  <%=i+1 %>
                </p>
              </th>
              <td><%=card[i].card_code%></td>
              <td><%=card[i].card_kind%></td>
              <td><%=card[i].card_period %></td>
              <td>
                <div class="btn-group" role="group" aria-label="Basic mixed styles example">
                  <button type="button" class="btn btn-primary" style="font-size: small;"
                    onclick="location.href='/mypage/card/update/<%= card[i].card_uid %>'">수정</button>
                </div>
              </td>
              <td>
                <div class="btn-group" role="group" aria-label="Basic mixed styles example">
                  <form action='/mypage/card/delete/<%= card[i].card_uid %>?_method=DELETE' method='post'>
                    <button type="submit" class="btn btn-outline-primary" style="font-size: small;">삭제</button>
                  </form>
                </div>
              </td>
            </tr>
            <% } %>
        </tbody>
      </table>

      <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
        <button type="submit" class="btn btn-dark btn-lg px-4 gap-3"
          onclick="location.href='/mypage/card/insert'">추가</button>
      </div>
    </div>
  </div>

  <!-- Optional JavaScript; choose one of the two! -->

  <!-- Option 1: Bootstrap Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj"
    crossorigin="anonymous"></script>

